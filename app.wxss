/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
page{background:#F4F4F4;font-family: PingFangSC-Regular;}

.swift_button_wrap{position:absolute;bottom:60rpx;left:0;right:0;padding:0 30rpx;}
.swift_button{
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  color:#ffffff;font-size:36rpx;border-radius:10rpx;height:100rpx;line-height:100rpx;}
.swift_button.size_small{height:68rpx;line-height: 68rpx;font-size:28rpx;}
.swift_button_hover{opacity:0.95;}
.swift_button.roud{border-radius:100rpx;}